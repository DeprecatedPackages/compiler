{
    "name": "rector/prefixer",
    "description": "Creates prefixed version of Rector code for standalone use in version conflicting situations",
    "license": "MIT",
    "authors": [
        { "name": "Tomas Votruba", "email": "tomas.vot@gmail.com", "homepage": "https://tomasvotruba.com" }
    ],
    "require": {
        "php": "^7.1",
        "humbug/php-scoper": "^0.11",
        "nette/utils": "^2.5",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/process": "^4.1",
        "symplify/package-builder": "^5.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "symplify/easy-coding-standard": "^5.1",
        "tracy/tracy": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Prefixer\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check src",
        "fix-cs": "vendor/bin/ecs check src --fix",
        "phpstan": "vendor/bin/phpstan analyse src --level max"
    }
}
